#include <stdio.h>

void insertionSort(int arr[], int size) {
    int i, key, j;
    for (i = 1; i < size; i++) {
        key = arr[i];
        j = i - 1;
        while (j >= 0 && arr[j] > key) {
            arr[j + 1] = arr[j];
            j = j - 1;
        }
        arr[j + 1] = key;
    }
}

int linearSearch(int arr[], int size, int target) {
    int i;
    for (i = 0; i < size; i++) {
        if (arr[i] == target) {
            return i;
        }
    }
    return -1;
}

int main() {
    int n, i, cari, index;

    printf("Masukkan jumlah angka: ");
    scanf("%d", &n);

    int data[n];

    printf("Masukkan angka-angkanya:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &data[i]);
    }

    insertionSort(data, n);

    printf("Data setelah diurutkan: ");
    for (i = 0; i < n; i++) {
        printf("%d ", data[i]);
    }
    printf("\n");

    printf("Masukkan angka yang ingin dicari: ");
    scanf("%d", &cari);

    index = linearSearch(data, n, cari);

    if (index != -1) {
        printf("Angka ditemukan pada indeks: %d\n", index);
    } else {
        printf("Angka tidak ditemukan.\n");
    }

    return 0;
}
